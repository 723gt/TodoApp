<div class="header">
  <span class="appname">
    <label>
      Todo App
    </label>
  </span>
  <span class="logout">
    <%= link_to "LogOut",login_logout_url %>
  </span>
</div>
<h1 class="titles"> <%= @user %>'s Done Task </h1>
<%= link_to "To Todo",'/todo',:class => "ToTodo" %>
<%= link_to "To Doing",'/doing',:class => "DToDoing" %>
<% @todo.each do |data| %>
  <div class="donetask">
    <span class="outspan">
      <span class="donecont">
        Content:<%= data.content %>
      </span>
      <span class="donememo">
        Memo:<%= data.memo %>
      </span>
    </span>
    <span class="ddestroy">
      <%= link_to '削除' ,done_destroy_url(data) %>
    </span>
  </div>
<% end %>
